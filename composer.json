{
    "require": {
        "php": ">=5.3.6",
        "composer/installers": "1.0.*@dev",
        "ServiceRunner/li3_debug": "dev-master",
        "unionofrad/lithium": "dev-master",
        "unionofrad/li3_quality": "dev-master",
        "unionofrad/li3_docs": "dev-master",
        "unionofrad/li3_queue": "dev-master",
        "primetheus/li3_paginate": "dev-master",
        "tmaiaroto/li3_access": "dev-master",
        "unionofrad/manual": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ServiceRunner/li3_debug",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/ServiceRunner/li3_debug.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "unionofrad/framework",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/UnionOfRAD/framework.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "primetheus/li3_paginate",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/primetheus/li3_paginate.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "tmaiaroto/li3_access",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/tmaiaroto/li3_access.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "unionofrad/manual",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/UnionOfRAD/manual.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "unionofrad/li3_queue",
                "version": "dev-master",
                "type": "lithium-library",
                "source": {
                    "url": "https://github.com/UnionOfRAD/li3_queue.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "extra": {
        "installer-paths": {
            "htdocs": [
                "unionofrad/framework"
            ],
            "htdocs/libraries/{$name}/": [
                "tmaiaroto/li3_access",
                "primetheus/li3_paginate",
                "unionofrad/li3_queue",
                "unionofrad/li3_quality",
                "unionofrad/lithium",
                "unionofrad/manual",
                "unionofrad/li3_docs"
            ]
        }
    },
    "config": {
        "vendor-dir": "htdocs/libraries/"
    },
    "minimum-stability": "dev"
}
